<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:markup="clr-namespace:Crystal.Themes.Markup"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:options="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    mc:Ignorable="options">

  <!--  Matadata  -->
  <system:String x:Key="Theme.Name">{{ThemeName}}</system:String>
  <system:String x:Key="Theme.Origin">Crystal.Themes</system:String>
  <system:String x:Key="Theme.DisplayName">{{ThemeDisplayName}}</system:String>
  <system:String x:Key="Theme.BaseColorScheme">{{BaseColorScheme}}</system:String>
  <system:String x:Key="Theme.ColorScheme">{{ColorScheme}}</system:String>
  <Color x:Key="Theme.PrimaryAccentColor">{{Crystal.Colors.AccentBase}}</Color>
  <SolidColorBrush x:Key="Theme.ShowcaseBrush" Color="{{Crystal.Colors.AccentBase}}" options:Freeze="True"/>

  <!--  *************COLORS START*************  -->

  <!--  Theme Base Colors  -->

  <!--  ACCENT COLORS  -->
  <Color x:Key="Crystal.Colors.Highlight">{{Crystal.Colors.Highlight}}</Color>
  <Color x:Key="Crystal.Colors.AccentBase">{{Crystal.Colors.AccentBase}}</Color>
  <!--  80%  -->
  <Color x:Key="Crystal.Colors.Accent">{{Crystal.Colors.Accent}}</Color>
  <!--  60%  -->
  <Color x:Key="Crystal.Colors.Accent2">{{Crystal.Colors.Accent2}}</Color>
  <!--  40%  -->
  <Color x:Key="Crystal.Colors.Accent3">{{Crystal.Colors.Accent3}}</Color>
  <!--  20%  -->
  <Color x:Key="Crystal.Colors.Accent4">{{Crystal.Colors.Accent4}}</Color>

  <!--  BASE COLORS  -->
  <Color x:Key="Crystal.Colors.ThemeForeground">{{Crystal.Colors.ThemeForeground}}</Color>
  <Color x:Key="Crystal.Colors.ThemeBackground">{{Crystal.Colors.ThemeBackground}}</Color>
  <Color x:Key="Crystal.Colors.IdealForeground">{{Crystal.Colors.IdealForeground}}</Color>

  <Color x:Key="Crystal.Colors.Gray1">{{Crystal.Colors.Gray1}}</Color>
  <Color x:Key="Crystal.Colors.Gray2">{{Crystal.Colors.Gray2}}</Color>
  <Color x:Key="Crystal.Colors.Gray3">{{Crystal.Colors.Gray3}}</Color>
  <Color x:Key="Crystal.Colors.Gray4">{{Crystal.Colors.Gray4}}</Color>
  <Color x:Key="Crystal.Colors.Gray5">{{Crystal.Colors.Gray5}}</Color>
  <Color x:Key="Crystal.Colors.Gray6">{{Crystal.Colors.Gray6}}</Color>
  <Color x:Key="Crystal.Colors.Gray7">{{Crystal.Colors.Gray7}}</Color>
  <Color x:Key="Crystal.Colors.Gray8">{{Crystal.Colors.Gray8}}</Color>
  <Color x:Key="Crystal.Colors.Gray9">{{Crystal.Colors.Gray9}}</Color>
  <Color x:Key="Crystal.Colors.Gray10">{{Crystal.Colors.Gray10}}</Color>

  <Color x:Key="Crystal.Colors.Gray">{{Crystal.Colors.Gray}}</Color>
  <Color x:Key="Crystal.Colors.Gray.MouseOver">{{Crystal.Colors.Gray.MouseOver}}</Color>
  <Color x:Key="Crystal.Colors.Gray.SemiTransparent">{{Crystal.Colors.Gray.SemiTransparent}}</Color>

  <Color x:Key="Crystal.Colors.Flyout">{{Crystal.Colors.Flyout}}</Color>

  <!--  CORE CONTROL COLORS  -->
  <Color x:Key="Crystal.Colors.ProgressIndeterminate1">#33878787</Color>
  <Color x:Key="Crystal.Colors.ProgressIndeterminate2">#33959595</Color>
  <Color x:Key="Crystal.Colors.ProgressIndeterminate3">#4C000000</Color>
  <Color x:Key="Crystal.Colors.ProgressIndeterminate4">#4C000000</Color>

