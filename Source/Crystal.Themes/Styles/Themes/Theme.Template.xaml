  <SolidColorBrush x:Key="Crystal.Brushes.Accent4" Color="{StaticResource Crystal.Colors.Accent4}" options:Freeze="True"/>

  <SolidColorBrush x:Key="Crystal.Brushes.Gray1" Color="{StaticResource Crystal.Colors.Gray1}" options:Freeze="True"/>
  <SolidColorBrush x:Key="Crystal.Brushes.Gray2" Color="{StaticResource Crystal.Colors.Gray2}" options:Freeze="True"/>
  <SolidColorBrush x:Key="Crystal.Brushes.Gray3" Color="{StaticResource Crystal.Colors.Gray3}" options:Freeze="True"/>
  <SolidColorBrush x:Key="Crystal.Brushes.Gray4" Color="{StaticResource Crystal.Colors.Gray4}" options:Freeze="True"/>
  <SolidColorBrush x:Key="Crystal.Brushes.Gray5" Color="{StaticResource Crystal.Colors.Gray5}" options:Freeze="True"/>
  <SolidColorBrush x:Key="Crystal.Brushes.Gray6" Color="{StaticResource Crystal.Colors.Gray6}" options:Freeze="True"/>
  <SolidColorBrush x:Key="Crystal.Brushes.Gray7" Color="{StaticResource Crystal.Colors.Gray7}" options:Freeze="True"/>
  <SolidColorBrush x:Key="Crystal.Brushes.Gray8" Color="{StaticResource Crystal.Colors.Gray8}" options:Freeze="True"/>
  <SolidColorBrush x:Key="Crystal.Brushes.Gray9" Color="{StaticResource Crystal.Colors.Gray9}" options:Freeze="True"/>
  <SolidColorBrush x:Key="Crystal.Brushes.Gray10" Color="{StaticResource Crystal.Colors.Gray10}" options:Freeze="True"/>

  <SolidColorBrush x:Key="Crystal.Brushes.Gray" Color="{StaticResource Crystal.Colors.Gray}" options:Freeze="True"/>
  <SolidColorBrush x:Key="Crystal.Brushes.Gray.MouseOver" Color="{StaticResource Crystal.Colors.Gray.MouseOver}" options:Freeze="True"/>
