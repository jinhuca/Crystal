  <SolidColorBrush x:Key="Crystal.Brushes.Thumb" Color="{StaticResource Crystal.Colors.Gray5}" options:Freeze="True"/>

  <LinearGradientBrush x:Key="Crystal.Brushes.Progress" StartPoint="1.002,0.5" EndPoint="0.001,0.5" options:Freeze="True">
