<UserControl x:Class="C3201ModuleSubscriber.Views.SubscriberView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:crystal="http://schemas.crystal.com"
             crystal:ViewModelLocator.AutoWireViewModel="True"
             HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
  <Border BorderBrush="DarkBlue" BorderThickness="2" CornerRadius="2" Padding="5" Margin="10">
    <ListBox ItemsSource="{Binding Messages}"/>
  </Border>
</UserControl>
