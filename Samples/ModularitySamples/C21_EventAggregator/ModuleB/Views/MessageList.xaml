<UserControl x:Class="ModuleB.Views.MessageList"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:crystal="http://schemas.crystal.com"
             crystal:ViewModelLocator.AutoWireViewModel="True" Padding="25">
  <Border BorderBrush="DarkBlue" BorderThickness="3" CornerRadius="3">
    <ListBox ItemsSource="{Binding Messages}"/>
  </Border>
</UserControl>
